{
  "name": "mimmi20/laminasviewrenderer-flash-message",
  "description": "Flash-Viewhelper",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminasviewrenderer-flash-message",
  "support": {
    "issues": "https://github.com/mimmi20/laminasviewrenderer-flash-message/issues",
    "source": "https://github.com/mimmi20/laminasviewrenderer-flash-message"
  },
  "require": {
    "php": "^8.1.0",
    "laminas/laminas-mvc-plugin-flashmessenger": "^1.10.1",
    "laminas/laminas-view": "^2.34.0",
    "psr/container": "^1.1.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.9",
    "laminas/laminas-modulemanager": "^2.15.0",
    "laminas/laminas-servicemanager": "^3.22.1",
    "mimmi20/coding-standard": "^5.2.13",
    "nikic/php-parser": "^v4.18.0",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "1.10.58",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.15",
    "phpunit/phpunit": "^10.5.10",
    "rector/rector": "^1.0.0"
  },
  "suggest": {
    "laminas/laminas-modulemanager": "to register this project as module",
    "laminas/laminas-servicemanager": "to use the navigation factories"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\LaminasView\\FlashMessage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20\\LaminasView\\FlashMessage\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\LaminasView\\FlashMessage",
      "config-provider": "Mimmi20\\LaminasView\\FlashMessage\\ConfigProvider"
    }
  }
}
